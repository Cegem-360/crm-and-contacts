{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "dcrepper/filament-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-dompdf": "^3.1",
        "filament/filament": "^4.7.1",
        "google/apiclient": "^2.19.0",
        "laravel/framework": "^12.52.0",
        "laravel/reverb": "^1.7.1",
        "laravel/sanctum": "^4.3.1",
        "laravel/tinker": "^2.11.1",
        "livewire/livewire": ">=3.7.10",
        "madbox-99/laravel-user-team-sync": "dev-main",
        "nunomaduro/essentials": "@dev",
        "spatie/laravel-activitylog": "^4.11.0",
        "spatie/laravel-permission": "^6.24.1",
        "typesense/typesense-php": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6.1",
        "driftingly/rector-laravel": "^2.1.9",
        "fakerphp/faker": "^1.24.1",
        "filament/blueprint": ">=1.1",
        "filament/upgrade": "^4.7.1",
        "laravel/boost": ">=2.1.7",
        "laravel/pail": "^1.2.6",
        "laravel/pint": "^1.27.1",
        "laravel/sail": "^1.53",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.9.1",
        "pestphp/pest": ">=4.4.1",
        "pestphp/pest-plugin-browser": "^4.3.0",
        "pestphp/pest-plugin-faker": ">=4",
        "pestphp/pest-plugin-laravel": ">=4",
        "pestphp/pest-plugin-livewire": ">=4.1.0",
        "rector/rector": ">=2.3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "update:requirements": [
            "composer bump",
            "npx npm-check-updates -u"
        ],
        "lint": [
            "@php vendor/bin/rector",
            "@php vendor/bin/pint --parallel",
            "npm run lint"
        ],
        "test:lint": [
            "@php vendor/bin/pint --parallel",
            "@php vendor/bin/rector --dry-run",
            "npm run test:lint"
        ],
        "test:unit": "pest --parallel",
        "test": [
            "@test:unit",
            "@test:lint"
        ],
        "fix": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/rector process app --ansi",
            "@php vendor/bin/rector process database --ansi",
            "@php vendor/bin/rector process routes --ansi",
            "@php vendor/bin/rector process tests --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "bump-after-update": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "name": "filament",
            "type": "composer",
            "url": "https://packages.filamentphp.com/composer"
        }
    ]
}
